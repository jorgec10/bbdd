CREATE OR REPLACE TRIGGER T_ONG_1
  AFTER
    INSERT OR UPDATE OF ONG
    ON PARTICIPACION
  REFERENCING NEW AS NEW
  FOR EACH ROW
BEGIN
  IF (NEW.ONG MEMBER SELECT P.ONG
                FROM TRABAJADOR T, PARTICIPACION P
                WHERE P.TRABAJADOR=T.DNI AND P.ONG <> T.ONG) THEN
    DBMS_OUTPUT.PUT_LINE('Esta igual colega');
END;